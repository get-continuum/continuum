{
  "name": "@get-continuum/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Continuum (semantic resolution)",
  "license": "Apache-2.0",
  "author": "Continuum",
  "repository": {
    "type": "git",
    "url": "https://github.com/get-continuum/continuum"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
